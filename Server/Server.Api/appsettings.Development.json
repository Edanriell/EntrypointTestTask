{
	"ConnectionStrings": {
		"Database": "Host=server-db;Port=5432;Database=server;Username=postgres;Password=postgres;",
		"Cache": "server-redis:6379"
	},
	"Serilog": {
		"Using": [
			"Serilog.Sinks.Console",
			"Serilog.Sinks.Seq"
		],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "Seq",
				"Args": {
					"serverUrl": "http://server-seq:5341"
				}
			}
		],
		"Enrich": [
			"FromLogContext",
			"WithMachineName",
			"WithThreadId"
		]
	},
	"Authentication": {
		"Audience": "account",
		"ValidIssuer": "http://server-idp:8080/realms/server",
		"MetadataUrl": "http://server-idp:8080/realms/server/.well-known/openid-configuration",
		"RequireHttpsMetadata": false
	},
	"Keycloak": {
		"BaseUrl": "http://server-idp:8080",
		"AdminUrl": "http://server-idp:8080/admin/realms/server/",
		"TokenUrl": "http://server-idp:8080/realms/server/protocol/openid-connect/token",
		"AdminClientId": "server-admin-client",
		"AdminClientSecret": "UZDmbNxWmV4TlpaCRcju6pMRsyuV3er1",
		"AuthClientId": "server-auth-client",
		"AuthClientSecret": "3E3yvXaYppoYBF3Ir6DgtEzADKKzSurZ"
	}
}  
